<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:BTimeLogger.Wpf.Controls"
                    xmlns:windows="clr-namespace:BTimeLogger.Wpf.Windows"
                   >
    <DataTemplate DataType="{x:Type controls:HomeViewModel}">
        <controls:Home ViewModel="{Binding }" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type controls:IntervalsViewModel}">
        <controls:Intervals ViewModel="{Binding }" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type controls:StatisticsViewModel}">
        <controls:Statistics ViewModel="{Binding }" />
    </DataTemplate>
</ResourceDictionary>